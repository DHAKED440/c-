#include<iostream>
#include<vector>

using namespace std;

int main () {

 int n,key;
 cout << "Enter the value of n : " ;
 cin >> n;

 vector<int>num;
 for(int i=0; i<n; i++){
   // cin >> num[i];
   int x;
   cin >> x;
   num.push_back(x);
 }

 cout << "Enter the key : ";
 cin >> key;
  for(int i=0; i<n; i++){
   if(num[i] == key){
      cout << "key is present" << endl;
      return 0;
   }
  }

  cout << "key is absent" << endl;
 return 0;
}
